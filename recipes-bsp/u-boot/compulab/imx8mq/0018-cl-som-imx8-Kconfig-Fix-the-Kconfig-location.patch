From 75af1a2b0ff6f4dd80dd1ef883cd26a78afca786 Mon Sep 17 00:00:00 2001
From: Valentin Raevsky <valentin@compulab.co.il>
Date: Thu, 5 Sep 2019 14:26:56 +0300
Subject: [PATCH 18/32] cl-som-imx8: Kconfig: Fix the Kconfig location

Signed-off-by: Valentin Raevsky <valentin@compulab.co.il>
---
 arch/arm/mach-imx/imx8m/Kconfig    |  2 +-
 board/compulab/cl-som-imx8/Kconfig | 48 ++++++++++++++++++++++++++++++++++++++
 board/compulab/imx8/Kconfig        | 48 --------------------------------------
 3 files changed, 49 insertions(+), 49 deletions(-)
 create mode 100644 board/compulab/cl-som-imx8/Kconfig
 delete mode 100644 board/compulab/imx8/Kconfig

diff --git a/arch/arm/mach-imx/imx8m/Kconfig b/arch/arm/mach-imx/imx8m/Kconfig
index bebdde9..c64ae51 100644
--- a/arch/arm/mach-imx/imx8m/Kconfig
+++ b/arch/arm/mach-imx/imx8m/Kconfig
@@ -82,6 +82,6 @@ source "board/freescale/imx8mq_phanbell/Kconfig"
 source "board/freescale/imx8mq_aiy/Kconfig"
 source "board/freescale/imx8mm_evk/Kconfig"
 source "board/freescale/imx8mm_val/Kconfig"
-source "board/compulab/imx8/Kconfig"
+source "board/compulab/cl-som-imx8/Kconfig"
 
 endif
diff --git a/board/compulab/cl-som-imx8/Kconfig b/board/compulab/cl-som-imx8/Kconfig
new file mode 100644
index 0000000..36ee7b2
--- /dev/null
+++ b/board/compulab/cl-som-imx8/Kconfig
@@ -0,0 +1,48 @@
+if TARGET_COMPULAB_IMX8
+
+choice
+    prompt "CompuLab iMX8 board select"
+
+config TARGET_CL_SOM_IMX8
+    bool "cl-som-imx8"
+
+endchoice
+
+config DEFAULT_DTB
+    string "U-Boot Default Device Tree File"
+    depends on TARGET_COMPULAB_IMX8
+    default "sbc-imx8.dtb"
+
+config SYS_BOARD
+	default "cl-som-imx8"
+
+config SYS_VENDOR
+	default "compulab"
+
+config SYS_CONFIG_NAME
+	default "cl-som-imx8"
+
+choice
+	prompt "RAM Volume select"
+	default RAM_BOARD_CFG
+	help
+	 This option configures 2G RAM only
+
+config RAM_1G
+	bool "RAM_1G"
+	help
+	 This option configures 1G RAM only
+
+config RAM_2G
+	bool "RAM_2G"
+	help
+	 This option configures 2G RAM only
+
+config RAM_BOARD_CFG
+	bool "RAM_BOARD_CFG"
+	help
+	 Read board configuration GPIO for RAM size
+
+endchoice
+
+endif
diff --git a/board/compulab/imx8/Kconfig b/board/compulab/imx8/Kconfig
deleted file mode 100644
index 36ee7b2..0000000
--- a/board/compulab/imx8/Kconfig
+++ /dev/null
@@ -1,48 +0,0 @@
-if TARGET_COMPULAB_IMX8
-
-choice
-    prompt "CompuLab iMX8 board select"
-
-config TARGET_CL_SOM_IMX8
-    bool "cl-som-imx8"
-
-endchoice
-
-config DEFAULT_DTB
-    string "U-Boot Default Device Tree File"
-    depends on TARGET_COMPULAB_IMX8
-    default "sbc-imx8.dtb"
-
-config SYS_BOARD
-	default "cl-som-imx8"
-
-config SYS_VENDOR
-	default "compulab"
-
-config SYS_CONFIG_NAME
-	default "cl-som-imx8"
-
-choice
-	prompt "RAM Volume select"
-	default RAM_BOARD_CFG
-	help
-	 This option configures 2G RAM only
-
-config RAM_1G
-	bool "RAM_1G"
-	help
-	 This option configures 1G RAM only
-
-config RAM_2G
-	bool "RAM_2G"
-	help
-	 This option configures 2G RAM only
-
-config RAM_BOARD_CFG
-	bool "RAM_BOARD_CFG"
-	help
-	 Read board configuration GPIO for RAM size
-
-endchoice
-
-endif
-- 
