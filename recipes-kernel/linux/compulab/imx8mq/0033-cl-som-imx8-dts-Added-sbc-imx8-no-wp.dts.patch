Subject: [PATCH 33/33] cl-som-imx8: dts: Added sbc-imx8-no-wp.dts

Signed-off-by: Valentin Raevsky <valentin@compulab.co.il>
---
 arch/arm64/boot/dts/compulab/Makefile           |  1 +
 arch/arm64/boot/dts/compulab/sbc-imx8-no-wp.dts | 31 +++++++++++++++++++++++++
 2 files changed, 32 insertions(+)
 create mode 100644 arch/arm64/boot/dts/compulab/sbc-imx8-no-wp.dts

diff --git a/arch/arm64/boot/dts/compulab/Makefile b/arch/arm64/boot/dts/compulab/Makefile
index 7db3b6f..ea53ccd 100644
--- a/arch/arm64/boot/dts/compulab/Makefile
+++ b/arch/arm64/boot/dts/compulab/Makefile
@@ -8,6 +8,7 @@ dtb-$(CONFIG_ARCH_FSL_IMX8MQ) += sbc-imx8-dual.dtb
 dtb-$(CONFIG_ARCH_FSL_IMX8MQ) += sbc-imx8-no-edid.dtb
 dtb-$(CONFIG_ARCH_FSL_IMX8MQ) += sbc-imx8-dp.dtb
 dtb-$(CONFIG_ARCH_FSL_IMX8MQ) += sbc-imx8-m4.dtb
+dtb-$(CONFIG_ARCH_FSL_IMX8MQ) += sbc-imx8-no-wp.dtb
 
 always		:= $(dtb-y)
 subdir-y	:= $(dts-dirs)
diff --git a/arch/arm64/boot/dts/compulab/sbc-imx8-no-wp.dts b/arch/arm64/boot/dts/compulab/sbc-imx8-no-wp.dts
new file mode 100644
